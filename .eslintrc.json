{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base", "eslint-config-prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "eslint-plugin-prettier"
    ],
    "rules": {
        "global-require":"off",
        "no-unused-vars": "warn",
        "prettier/prettier":"error",
        "func-names": "off",
        "class-methods-use-this": "off",
        "no-param-reassign":"off",
        "import/extensions": "off",
        "no-underscore-dangle":"off"
    }
}
